{
  "version": 2,
  "builds": [
    {
      "use": "@vercel/static-build",
      "entrypoint": "build/index.html",
      "src": "build/**",
      "config": {
        "clean_urls": true
      }
    },
    {
      "src": "/api/.*",
      "use": "@vercel/node",
      "config": {
        "memory": 128
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ]
}
